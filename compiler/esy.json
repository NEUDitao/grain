{
  "name": "@grain/compiler",
  "version": "0.5.1",
  "esy": {
    "build": [
      "dune build @native --no-buffer"
    ],
    "buildEnv": {
      "DUNE_BUILD_DIR": "#{self.target_dir}",
      "GRAIN_STDLIB": "#{self.root / '..' / 'stdlib'}",
      "GRAINC_BIN_PATH": "#{self.target_dir / 'default' / 'grainc' / 'grainc.exe'}",
      "GRAINC_JS_PATH": "#{self.target_dir / 'default' / 'grainc' / 'grainc.bc.js'}",
      "GRAINDOC_BIN_PATH": "#{self.target_dir / 'default' / 'graindoc' / 'graindoc.exe'}",
      "GRAINDOC_JS_PATH": "#{self.target_dir / 'default' / 'graindoc' / 'graindoc.bc.js'}",
      "GRAINFORMAT_BIN_PATH": "#{self.target_dir / 'default' / 'grainformat' / 'grainformat.exe'}",
      "GRAINFORMAT_JS_PATH": "#{self.target_dir / 'default' / 'grainformat' / 'grainformat.bc.js'}",
      "GRAINLSP_BIN_PATH": "#{self.target_dir / 'default' / 'grainlsp' / 'grainlsp.exe'}",
      "GRAINLSP_JS_PATH": "#{self.target_dir / 'default' / 'grainlsp' / 'grainlsp.bc.js'}",
      "CLI_BIN_DIR": "#{self.root / '..' / 'cli' / 'bin'}",
      "TEST_EXEC_PATH": "#{self.target_dir / 'default' / 'test' / 'test.exe'}",
      "TEST_JS_PATH": "#{self.target_dir / 'default' / 'test' / 'test.bc.js'}"
    }
  },
  "scripts": {
    "copy:exe": "cp #{$GRAINC_BIN_PATH} #{$GRAINDOC_BIN_PATH} #{$GRAINFORMAT_BIN_PATH} #{$GRAINLSP_BIN_PATH} #{$CLI_BIN_DIR}",
    "copy:js": "cp #{$GRAINC_JS_PATH} #{$GRAINDOC_JS_PATH} #{$GRAINFORMAT_JS_PATH} #{$GRAINLSP_JS_PATH}  #{$CLI_BIN_DIR}",
    "clean": "rm -rf #{self.root}/_esy",
    "test": "#{$TEST_EXEC_PATH}",
    "test:js": "node #{$TEST_JS_PATH}",
    "format": "dune build @fmt --auto-promote",
    "check-format": "dune build @fmt"
  },
  "dependencies": {
    "@grain/binaryen.ml": ">= 0.17.0 < 0.18.0",
    "@opam/cmdliner": ">= 1.0.2",
    "@opam/dune": ">= 3.0.0 < 4.0.0",
    "@opam/dune-build-info": ">= 3.0.0 < 4.0.0",
    "@opam/dune-configurator": ">= 3.0.0 < 4.0.0",
    "@opam/fp": "0.0.1",
    "@opam/fs": "0.0.2",
    "@opam/menhir": "20211125",
    "@opam/ocamlgraph": ">= 2.0.0",
    "@opam/ppx_deriving": ">= 5.2.1",
    "@opam/ppx_deriving_cmdliner": ">= 0.6.0",
    "@opam/ppx_deriving_yojson": ">= 3.5.2",
    "@opam/ppx_sexp_conv": ">= 0.14.0",
    "@opam/reason": ">= 3.8.0",
    "@opam/sexplib": ">= 0.14.0",
    "@opam/uri": ">= 4.2.0 < 5.0.0",
    "@opam/utf8": "0.1.0",
    "@opam/yojson": ">= 1.7.0",
    "ocaml": "4.14.0"
  },
  "devDependencies": {
    "@opam/js_of_ocaml-compiler": ">= 4.0.0 < 5.0.0",
    "@opam/ocaml-lsp-server": "1.11.6",
    "@reason-native/rely": "^4.0.0"
  },
  "resolutions": {
    "ocaml": "4.14.0",
    "@opam/reason": "reasonml/reason:reason.opam#4f6ff7616bfa699059d642a3d16d8905d83555f6",
    "@opam/fp": "grain-lang/reason-native:fp.opam#8d2b9c3cbf1d0a4074f34116e7e92ccb00b040f6",
    "@opam/fs": "grain-lang/reason-native:fs.opam#8d2b9c3cbf1d0a4074f34116e7e92ccb00b040f6",
    "@opam/utf8": "grain-lang/reason-native:utf8.opam#8d2b9c3cbf1d0a4074f34116e7e92ccb00b040f6",
    "@reason-native/cli": "grain-lang/reason-native:cli.json#8d2b9c3cbf1d0a4074f34116e7e92ccb00b040f6",
    "@reason-native/rely": "grain-lang/reason-native:rely.json#8d2b9c3cbf1d0a4074f34116e7e92ccb00b040f6",
    "@reason-native/pastel": "grain-lang/reason-native:pastel.json#8d2b9c3cbf1d0a4074f34116e7e92ccb00b040f6",
    "@reason-native/file-context-printer": "grain-lang/reason-native:file-context-printer.json#8d2b9c3cbf1d0a4074f34116e7e92ccb00b040f6"
  },
  "installConfig": {
    "pnp": false
  }
}
